<% provide(:title, 'Edit Bet Type') %>
<div class = "itemForm">
<h1>Please enter Bet Type details: </h1>
<div class = "row">
	<div class = "span3 offset1">
		<%=form_for(@bet_type) do |f| %>
			<%=render 'shared/error_messages_bet_types' %>
			<%= f.label :name %>
			<%= f.text_field :name %>

			
			<br><br>
			<%=f.submit "Save changes", class: "btn btn-large btn-primary" %>
			<%end%>
		</div>
	</div>	
</div>